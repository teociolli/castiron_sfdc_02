<?xml version="1.0" encoding="UTF-8" standalone="yes"?><userScript xmlns="http://www.approuter.com/schemas/2003/1/UserCallouts/"><functionName>addRICE</functionName><arguments><argument required="true" type="number" name="tot"/><argument required="true" type="number" name="imp"/><argument required="true" type="string" name="direction"/><argument required="true" type="string" name="sign"/></arguments><result type="number"/><body>if (direction != 'RICE')
  imp = 0;

if (sign != '+')
   imp = -imp;

return tot + imp;
</body></userScript>